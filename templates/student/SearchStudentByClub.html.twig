{{ form(formsearch) }}
<h1>List of Student</h1>
<table border="1">
    <tr>
        <td>NSC</td>
        <td>Email</td>
        <td>Classe</td>
        <td>Action</td>
    </tr>
    {% for student in students %}
        <tr>
            <td>{{ student.nsc }}</td>
            <td>{{ student.email }}</td>
            <td>{{ student.classroom.name }}</td>
            <td><a href="{{ path('updateStudent',{'id':student.nsc}) }}">Update</a>
                <a href="{{ path('deleteStudent',{'id':student.nsc}) }}">Delete</a>
                <a href="{{ path('showStudent',{'id':student.nsc}) }}">Show</a></td>
        </tr>
    {% endfor %}
</table>

   {% for club in clubs %}
       <h1>Detail of {{ club.ref }}</h1>
       <ul>
            <li>Date of creation:  {{ club.creationDate|date }} </li>
           <li>fees:  {{ club.fees }}</li>
           <li>Enabled: {{ club.enabled }}</li>
       </ul>
   {% endfor %}
</table>